{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-react-hooks",
    "tslint-config-prettier"
  ],
  "rules": {
    "interface-name": false,
    "jsx-no-lambda": false,
    "no-empty": [true, "allow-empty-catch", "allow-empty-functions"],
    "prefer-for-of": false,
    "radix": false
  }
}
