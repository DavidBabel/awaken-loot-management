{
  "name": "awaken-loot-management",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "loot": "node db/json-to-sql/from-json-to-base.js",
    "types": "postgraphile -c test --no-server --export-schema-graphql schema.gql && graphql-codegen --config gen-types-config.yml && rm schema.gql && sed -i.bak '1d' lib/generatedTypes.tsx && rm lib/generatedTypes.tsx.bak",
    "db": "node server/index.js",
    "dbw": "nodemon server/index.js",
    "reset": "cat db/*.sql > db/gen/db.sql && (dropdb test || true) && createdb test && psql test < db/gen/db.sql"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.0.1",
    "@apollo/react-ssr": "^3.0.1",
    "@material-ui/core": "^4.4.0",
    "@zeit/next-sass": "^1.0.1",
    "apollo-boost": "^0.4.4",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^14.5.3",
    "isomorphic-unfetch": "3.0.0",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "next": "^9.0.5",
    "next-routes": "^1.4.2",
    "node-sass": "^4.12.0",
    "nookies": "^2.0.8",
    "postgraphile": "^4.4.3",
    "react": "^16.7.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.6.1",
    "@graphql-codegen/introspection": "1.6.1",
    "@graphql-codegen/typescript": "1.6.1",
    "@graphql-codegen/typescript-operations": "1.6.1",
    "@graphql-codegen/typescript-react-apollo": "1.6.1",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^24.0.18",
    "@types/next": "^8.0.6",
    "@types/node": "^12.7.2",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "jest": "^24.9.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  }
}
