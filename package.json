{
  "name": "awaken-loot-management",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "loot": "node db/json-to-sql/from-json-to-base.js",
    "types": "postgraphile -c test --no-server --export-schema-graphql schema.gql && graphql-codegen --config gen-types-config.yml && rm schema.gql",
    "db": "postgraphile -c test --enhance-graphiql",
    "reset": "cat db/*.sql > db/gen/db.sql && (dropdb test || true) && createdb test && psql test < db/gen/db.sql"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.0.1",
    "@apollo/react-ssr": "^3.0.1",
    "apollo-boost": "^0.4.4",
    "graphql": "^14.5.1",
    "isomorphic-unfetch": "3.0.0",
    "next": "^9.0.4",
    "postgraphile": "^4.4.3",
    "react": "^16.7.0",
    "react-admin": "^2.9.6",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.6.1",
    "@graphql-codegen/introspection": "1.6.1",
    "@graphql-codegen/typescript": "1.6.1",
    "@graphql-codegen/typescript-operations": "1.6.1",
    "@graphql-codegen/typescript-react-apollo": "1.6.1",
    "@types/graphql": "^14.5.0",
    "@types/next": "^8.0.6",
    "@types/node": "^12.7.2",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "typescript": "^3.5.3"
  }
}
